		}},
         "format": "{{icon}}",
         "format-icons": {{
             "1": "Óûï",
             "2": "Ôáº",
             "3": "Û∞†Æ",
             "4": "Ôáø",
             "5": "Ôâ©",
             "6": "Û∞ìì",
             "7": "Û∞éÜ",
             "8": "Ó™º",
             "9": "Ó™º",
             "10": "Û∞çπ",
             "urgent": "Û∞Ä®",
             "focused": "ÔÑë",
             "default": "Ó™º",
         }},
