    }},
	"custom/weather#temp": {{
		"format": "{{}}",
		"tooltip": true,
		"interval": 3600,
		"exec": "wttrbar --location ararangu√° --hide-conditions --custom-indicator  '{{temp_C}}'",
		"return-type": "json"
	}},
	"custom/weather#icon": {{
