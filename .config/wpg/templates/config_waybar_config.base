    }},
	"group/pulse":{{
        "tooltip": false,
		"orientation": "inherit",

		"drawer":{{
			"children-class":"pulsecontainer",
			"transition-left-to-right":false,
			}},
		"modules":["pulseaudio#icon","pulseaudio#sound","pulseaudio/slider"]
		}},
	"pulseaudio/slider": {{
		"min": 0,
		"max": 100,
		"orientation": "vertical"
	}},
    "pulseaudio#sound": {{
        "format": "{{volume}}",
        "format-bluetooth": "{{volume}}%",
        "format-bluetooth-muted": "off",
        "format-muted": "off",
        "format-source-muted": "!mic",
        "format-icons": {{
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "Û∞ïæ"]
        }},
        "on-click": "hyprctl dispatch 'exec [float;noanim;move 3% 58%;size 40% 40%] GTK_THEME=FlatColor2 pavucontrol'",
        "on-click-middle" : "pulseaudio-control --node-blacklist 'alsa_output.pci-0000_01_00.1.hdmi-stereo-extra2' next-node",
        "on-click-right":"pulseaudio-control togmute",
    }},
	"pulseaudio#icon": {{
        "format": "{{icon}}",
        "format-bluetooth": "{{icon}}<sub>Ôäî</sub>",
        "format-bluetooth-muted": "Û∞ùü<sub>Ôäî</sub>",
        "format-muted": "Û∞ùü",
        "format-source-muted": "ÔÑ±",
        "format-icons": {{
            "headphone": "ÔÄ•",
            "hands-free": "Û±°è",
            "headset": "Û∞ãé",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "Û∞ïæ"]
        }},
        "on-click": "hyprctl dispatch 'exec [float;noanim;move 3% 58%;size 40% 40%] GTK_THEME=FlatColor2 pavucontrol'",
        "on-click-middle" : "pulseaudio-control --node-blacklist 'alsa_output.pci-0000_01_00.1.hdmi-stereo-extra2' next-node",
        "on-click-right":"pulseaudio-control togmute",
    }},
    "custom/dunst": {{
