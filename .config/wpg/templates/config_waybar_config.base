		"tooltip": false,
		"format": "{{icon}}",
		"format-icons": {{
			"notification": "Û±•Å",
			"none": "Û∞ç°",
			"default": "Û∞ç°",
			"dnd-notification": "Û±ôç",
			"dnd-none": "Û±ôç",
