    //"height": , // Waybar height (to be removed for auto height)
    "spacing": 0, // Gaps between modules (4px)
    "margin-left":0,
    "margin-right":0,
    //"margin-top":{gaps_out},
    //"margin-bottom":{gaps_out},

    // Choose the order of the modules
    "modules-left": [
		"custom/menu",
		"hyprland/workspaces" 
			],
    "modules-center": [ 
		"custom/weather#icon",
		"custom/weather#temp",
		"clock",
	], 

    "modules-right": [   
		"group/traybox",
		"group/pulse",
		"battery",
		//"custom/notification#count",
